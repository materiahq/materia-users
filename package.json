{
  "name": "@materia/users",
  "version": "1.0.0",
  "description": "Manage your users and permissions in your application",
  "keywords": [
    "auth",
    "user management",
    "materia designer"
  ],
  "main": "bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thyb/materia-users"
  },
  "author": "thyb@oauth.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thyb/materia-users/issues"
  },
  "homepage": "https://github.com/thyb/materia-users#readme",
  "scripts": {
    "build": "npm run clean && npm run build:client && ncp server dist/server && npm run build:server && npm run postbuild",
    "build:client": "rollup -c",
    "build:server": "tsc -p ./tsconfig.server.json",
    "postbuild": "rimraf dist/server/index.ts && ncp package.json dist/package.json",
    "clean": "rimraf dist",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "ng lint --type-check=true",
    "lint:sass": "sass-lint --verbose --no-exit --config ./.sass-lint.yml",
    "postinstall": "npm rebuild node-sass"
  },
  "dependencies": {
    "@angular/animations": "^6.0.5",
    "@angular/cdk": "^6.3",
    "@angular/common": "^6.0.5",
    "@angular/core": "^6.0.5",
    "@angular/forms": "^6.0.5",
    "@angular/material": "^6.3",
    "express-session": "^1.14.1",
    "flash": "^1.1.0",
    "md5": "^2.2.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "rxjs": "^6.2.1"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.8",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.55.1",
    "rollup-plugin-angular-inline": "^1.0.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-typescript2": "^0.10.0",
    "typescript": "^2.6.2"
  },
  "materia": true
}
