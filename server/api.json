[
	{
		"method": "post",
		"url": "/user/signin",
		"controller": "default",
		"action": "signin",
		"params": [
			{
				"name": "login",
				"type": "text",
				"required": true,
				"component": "input"
			},
			{
				"name": "password",
				"type": "text",
				"required": true,
				"component": "password"
			}

		]
	},
	{
		"method": "post",
		"url": "/user/logout",
		"controller": "default",
		"action": "logout",
		"permissions": [
			"Authenticated"
		]
	},
	{
		"method": "get",
		"url": "/user/me",
		"controller": "default",
		"action": "me",
		"permissions": [
			"Authenticated"
		]
	},
	{
		"method": "put",
		"url": "/user/me",
		"controller": "default",
		"action": "safeUpdate",
		"permissions": [
			"Authenticated"
		]
	},
	{
		"method": "put",
		"url": "/user/me/password",
		"controller": "default",
		"action": "changePassword",
		"params": [
			{
				"name": "new_password",
				"type": "text",
				"required": true
			}, {
				"name": "old_password",
				"type": "text",
				"required": false
			}, {
				"name": "key",
				"type": "text",
				"required": false
			}, {
				"name": "id_user",
				"type": "number",
				"required": false
			}
		]
	},
	{
		"method": "delete",
		"url": "/user/me",
		"controller": "default",
		"action": "destroy",
		"params": [{
			"name": "login",
			"type": "text",
			"required": "true"
		}],
		"permissions": [
			"Authenticated"
		]
	},
	{
		"method": "post",
		"url": "/user/signup",
		"controller": "default",
		"action": "signup",
		"permissions": [
			"Anyone"
		]
	}
]